{
    "name": "morningmedley/database",
    "description": "Illuminate Migrations and Eloquent for Morningmedley.",
    "keywords": [
        "wordpress",
        "database",
        "morningmedley"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Munk",
            "email": "mh@morningtrain.dk"
        }
    ],
    "autoload": {
        "psr-4": {
            "MorningMedley\\Database\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "morningmedley/application": "^0.2",
        "illuminate/database": "^12.3",
        "doctrine/dbal": "^3"
    },
    "extra": {
        "morningmedley": {
            "providers": [
                "MorningMedley\\Database\\DatabaseServiceProvider"
            ]
        }
    }
}
